{
    "version": 2,
    "builds": [
        {
            "src": "api/articulos.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/articulos",
            "dest": "api/articulos.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}